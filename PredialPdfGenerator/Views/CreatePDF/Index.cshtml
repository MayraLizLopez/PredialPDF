@model PredialPdfGenerator.Models.Kiosko
@{
    Layout = null;
}
<link href="~/css/estilos.css" rel="stylesheet" type="text/css">
<!--
/////////////////////////////////////////////////////////////////////////////
//////////////////////////////IMAGENES DE FONDO//////////////////////////////
/////////////////////////////////////////////////////////////////////////////
-->
<div id="invoice">
    <div id="contenedor">
        <img src="@Url.Content("~/img/predial1.svg")" alt="Predial1" id="predial1" />
        <img src="@Url.Content("~/img/predial3.svg")" alt="Predial2" id="predial2" />
    </div>
    <!--
    /////////////////////////////////////////////////////////////////////////////
    //////////////////////////DETALLES DEL CIUDADANO(A)//////////////////////////
    /////////////////////////////////////////////////////////////////////////////
    -->
    <div id="nombre">@Model.Nombre</div>
    <div id="direccion">@Model.Domicilio</div>
    <div id="clave">@Model.ClaveCatastral</div>
    <div id="fecha">@Model.Fecha</div>
    <!--
    /////////////////////////////////////////////////////////////////////////////
    ////////////////////////////DETALLE DE MI CUENTA/////////////////////////////
    /////////////////////////////////////////////////////////////////////////////
        -->
    <div id="corriente">@Model.Corriente</div>
    <div id="adicional">@Model.Adicional</div>
    <div id="recargos">@Model.Recargos</div>
    <div id="rezagos">@Model.Rezagos</div>
    <div id="adicionalRezago">@Model.AdicionalRezago</div>
    <div id="recargoRezago">@Model.RecargosRezago</div>
    <div id="multa">@Model.Multa</div>
    <div id="honorario">@Model.Honorario</div>
    <div id="ejecucion">@Model.Ejecucion</div>
    <div id="diferenciaConstruccion">@Model.DiferenciaConstruccion</div>
    <div id="descuento">@Model.Descuento</div>
    <div id="total">@Model.Total</div>
    <!--
    /////////////////////////////////////////////////////////////////////////////
    /////////////////////////¿HASTA CUÁNDO DEBO PAGAR?///////////////////////////
    /////////////////////////////////////////////////////////////////////////////
        -->
    <div id="fechaVencimiento">@Model.FechaVencimiento</div>
    <div id="fechaVencimientoTxt">@Model.FechaVencimientoTxt</div>
    <div id="lineaCaptura">@Model.LineaCaptura</div>
    <div id="referenciaOxxo">@Model.ReferenciaOxxo</div>
    <div id="total2">@Model.Total</div>

    <div id="contenedorCodigoBarra" style="text-align: center; position: absolute; left: 75%; top:130%;">
        <div id="barcodeTarget" class="barcodeTarget" style=" width: 100%;"></div>
        <h3><canvas id="canvasTarget" width=250 style="width: 100%;"></canvas></h3>
    </div>
</div>


<script src="~/js/jquery-2.1.4.min.js" type="text/javascript"></script>
<script src="~/js/tooltipster.bundle.min.js" type="text/javascript"></script>
<script src="~/js/jquery-3.3.1.min.js"></script>
<script src="~/js/select2.full.js"></script>
<script src="~/js/jquery-barcode.js"></script>
<script src="~/js/jquery-barcode.min.js"></script>
<script src="~/js/bootstrap.min.js"></script>
<script src="~/js/html2pdf.bundle.min.js"></script>
<script type="text/javascript" src="~/js/Index.js"></script>



